public class Trash
{
	public void maxOccur(String s)
	{
	    int[] i=new int[26];
	    
	   for(Character c1:s.toCharArray())
	   {
		   i[c1-'a']=i[c1-'a']+1;
	   }
	   
	  for(int i1:i)
	  {
		  if(i[i1]>i1)
		  {
			  max=i1;
		  }
	  }
	   
	}
	public static void main(String args[])
	{
		
		 String s="test";
		 Trash t=new Trash();
		 t.maxOccur(s);
	}
	
