package com.HackerRank;

public class LongestSubString {

	public static void main(String[] args) {
		//longest substring without repeating its characters.
		String s="abcdaab";
		int count=0;
		for(int i=0;i<s.length();i++)
		{
			while(s.charAt(i)!=s.charAt(i+1))
			{
				count++;
			}
		}
        
	}

}
