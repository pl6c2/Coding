package bigSum;

import java.util.Scanner;

public class BigSum {

	static long aVeryBigSum(long[] ar)
	{
		long sum=0;
//		for(int i=0;i<3;i++)
//		{
//			sum=sum+ar[i];
//		}
		for(long l:ar)
		{
			sum=sum+l;
		}
		return sum;
		
	}
	public static void main(String args[])
	{
		Scanner sc=new Scanner(System.in);
		long[] ar = new long[3];
		ar[0]=1000000001;
		ar[1]=1000000002;
		ar[2]=1000000003;
		long result = aVeryBigSum(ar);
		System.out.println(result);

	}
}

