package com.LinkedList;

class Node {
	int data;
	Node next;
	Node(int data)
	{
		this.data=data;
		this.next = null; 
	}
}

class LinkedList {
	Node head;
	
	public void insert(int data)
	{
		Node node = new Node(data);
		
		if(head==null)
		{
			head=node;
		}
		else
		{
			Node n=head;
			while(n!=null)
			{
				n=n.next;
			}
			n.next=node;
		}
		
	}
	
	
	public void show()
	{
		Node n1=head;
		while(n1.next!=null)
		{
			n1=n1.next;
			System.out.println(n1.data);
		}
	}
}
public class AddNodes {

	public static void main(String args[])
	{
		LinkedList l=new LinkedList();
		l.insert(4);
//		System.out.println(l.head.data);
		l.insert(2);
		l.insert(3);
		l.insert(4);
		l.show();
	}
}
