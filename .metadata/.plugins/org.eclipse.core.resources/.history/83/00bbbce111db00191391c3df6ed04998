package com.Strings;

public class StudentAttendenceRecord {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
         String s="PPALLL";
         int count=0,countl=0, temp=0;
         
         for(int i=0;i<s.length()-1;i++)
         {
        	 if(s.charAt(i)=='A')
        	 {
        		 count=count+1;
        	 }
        	
        	 else if(s.charAt(i)=='L')
        	 {
        		countl=countl+1;
        		
        	 }
         }
         
         if(count>2)
         {
        	 System.out.println("false");
         }
	}

}
